{
	"compilerOptions": {
		"target": "es2020",
		"strict": false,
		"preserveConstEnums": true,
		"noEmit": true,
		"sourceMap": false,
		"module": "es2015",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"compileOnSave": true,
	"include": ["**/*.ts", "**/*.js", "../../types/**/*.ts"],
	"exclude": ["node_modules"]
}
